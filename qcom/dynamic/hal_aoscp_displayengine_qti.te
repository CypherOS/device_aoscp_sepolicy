# Do not use add_service() as hal_graphics_composer_default may be the provider as well
allow hal_aoscp_displayengine_qti qdisplay_service:service_manager find;

binder_call(hal_aoscp_displayengine_qti, hal_graphics_composer_default)

# Allow DisplayEngine to access vendor display property
get_prop(hal_aoscp_displayengine_qti, vendor_display_prop)

# Allow DisplayEngine to access pps socket
unix_socket_connect(hal_aoscp_displayengine_qti, pps, mm-pp-daemon)