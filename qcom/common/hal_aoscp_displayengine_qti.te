type hal_aoscp_displayengine_qti, domain;
hal_server_domain(hal_aoscp_displayengine_qti, hal_aoscp_displayengine)

type hal_aoscp_displayengine_qti_exec, exec_type, vendor_file_type, file_type;
init_daemon_domain(hal_aoscp_displayengine_qti)

# Allow DisplayEngine HAL's default implementation to use vendor-binder service
vndbinder_use(hal_aoscp_displayengine_qti)

# Allow DisplayEngine to store files under /data/vendor/display and access them
allow hal_aoscp_displayengine_qti display_vendor_data_file:dir rw_dir_perms;
allow hal_aoscp_displayengine_qti display_vendor_data_file:file create_file_perms;
